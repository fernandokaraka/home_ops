{
  "name": "homeops",
  "version": "1.0.0",
  "main": "expo-router/entry",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "build:dev": "npx eas-cli build --profile development --platform all",
    "build:dev:android": "npx eas-cli build --profile development --platform android",
    "build:dev:ios": "npx eas-cli build --profile development --platform ios",
    "build:preview": "npx eas-cli build --profile preview --platform all",
    "build:preview:android": "npx eas-cli build --profile preview --platform android",
    "build:preview:ios": "npx eas-cli build --profile preview --platform ios",
    "build:prod": "npx eas-cli build --profile production --platform all",
    "build:prod:android": "npx eas-cli build --profile production --platform android",
    "build:prod:ios": "npx eas-cli build --profile production --platform ios",
    "submit:android": "npx eas-cli submit --platform android",
    "submit:ios": "npx eas-cli submit --platform ios",
    "lint": "expo lint",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@expo/vector-icons": "^15.0.3",
    "@react-native-async-storage/async-storage": "2.2.0",
    "@supabase/supabase-js": "^2.45.0",
    "expo": "^54.0.0",
    "expo-asset": "~12.0.12",
    "expo-constants": "~18.0.13",
    "expo-device": "~8.0.10",
    "expo-font": "~14.0.10",
    "expo-linking": "~8.0.11",
    "expo-notifications": "~0.32.16",
    "expo-router": "~6.0.21",
    "expo-status-bar": "~3.0.9",
    "nativewind": "^2.0.11",
    "react": "19.1.0",
    "react-native": "0.81.5",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-reanimated": "~4.1.1",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-url-polyfill": "^2.0.0",
    "react-native-worklets": "^0.7.1",
    "tailwindcss": "^3.3.2",
    "zustand": "^5.0.0"
  },
  "devDependencies": {
    "@types/react": "~19.1.10",
    "typescript": "~5.9.2"
  },
  "private": true
}
